{
	"manifest_version": 2,
	"name": "Syntero - Settings Sync",
	"version": "1.0.0",
	"description": "Synchronize Zotero settings and preferences across multiple devices",
	"author": "YU Shi Jiong (22107027D)",
	"homepage_url": "https://github.com/shijiongyu/syntero",
	"applications": {
		"zotero": {
			"id": "syntero@zotero.org",
			"update_url": "https://raw.githubusercontent.com/shijiongyu/syntero/master/updates.json",
			"strict_min_version": "6.999",
			"strict_max_version": "7.0.*"
		}
	},
	"permissions": [
		"storage",
		"notifications"
	]
}

